{
  "name": "@shopeasy/api",
  "version": "1.0.9",
  "description": "A NodeJS wrapper for Shop Easy API",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "wrapper",
    "shopeasy",
    "shop easy",
    "api",
    "typescript",
    "sales",
    "bot",
    "discord",
    "vendas"
  ],
  "author": {
    "name": "jeanbrnd",
    "url": "https://github.com/jeanbrnd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shopeasy-bot/sdk-api-js.git"
  },
  "bugs": {
    "url": "https://github.com/shopeasy-bot/sdk-api-js/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "tsx --env-file=.env playground/index.ts",
    "test": "vitest"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "unbuild": "^2.0.0",
    "vite-tsconfig-paths": "^5.1.0",
    "vitest": "^2.1.4"
  }
}
